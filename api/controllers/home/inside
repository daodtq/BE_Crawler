			/** GENERATED BY LOCALIZATION MODULE */
                var localePrefix = "";
                var localeIgnoreUri = '["dashboard\/report-by-order-localization","dashboard\/report-by-cost-localization","dashboard\/report-by-cost-estimate-localization","reports\/verifier\/find","gelato_regions.json","gelato_catalogs.json","printerval-uk\/upload","printerval-ca\/upload","printerval-au\/upload","printerval-fr\/upload","printerval-jp\/upload","printerval-it\/upload","printerval-de\/upload","printerval-es\/upload","printerval-pt\/upload","printerval-central\/upload","printerval-us\/upload","remove-size"]';
                var defaultLocale = "us";
                var ssoIsActive = "1";
                var logoutRoute = "https://printerval.com/sso/sso-logout";
                if (localePrefix) {
                    const originalFetch = window.fetch;
                    window.fetch = function() {
                        try {
                            var requestUrl = arguments[0];
                            var originUrl = new URL(window.location.href);
                            var isContainHostName = requestUrl && requestUrl.includes(originUrl.hostname);
                            //k phai domain khac
                            var isContainHttp = requestUrl && requestUrl.includes("http");
                            var isIgnoreLocalization = requestUrl && requestUrl.includes("ignore_localization=1");
                            if (!isIgnoreLocalization) {
                                if (isContainHostName) {
                                    requestUrl = requestUrl.replace(originUrl.hostname, originUrl.hostname + "/" + localePrefix);
                                } else if (!isContainHttp) {
                                    if (requestUrl.charAt(0) != "/") {
                                        requestUrl = "/" + requestUrl;
                                    }
                                    requestUrl = "/" + localePrefix + requestUrl;
                                }
                            }
                            arguments[0] = requestUrl;
                        } catch (err) {
                        }
                       return originalFetch.apply(null, arguments);
                    }
                }
                
