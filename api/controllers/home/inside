    function renderRecommendProducts(relatedProducts) {
        let locale = '';
        if (typeof localePrefix !== 'undefined' && localePrefix !== '') {
            locale = '/' + localePrefix;
        }
        let relatedProductDom = [];

        relatedProducts.forEach(element => 
        {
            let url = element.url;
            if (locale != "" && !element.url.includes(locale)) {
                url = locale + element.url;
            }

            relatedProductDom.push(`
                <div class="md-add2basket-item">
                    <a class="md-add2basket-link" href="${url}?internal_source=cart-popup-suggestion" target="_blank">
                        <img class="md-add2basket-image" src="${getImageCdn(element.image_url, 630, 630)}" alt="${element.name}" referrerpolicy="no-referrer">
                    </a>
                    <div class="md-add2basket-title-box flex-b align-c">
                        <a class="md-product-title" href="${url}?internal_source=cart-popup-suggestion" target="_blank">
                            ${element.name}
                        </a>
                    </div>
                    <span class="md-add2basket-price">
                        ${formatPrice(decorePrice(element.price), priceTemplate)}
                        ${getHighPriceHtml(element)}
                    </span>
                </div>
            `);

        });
                    $(".md-add2basket-list").slick('unslick');
        
        $(".md-add2basket-list").html(relatedProductDom.join(''));
        
                    $(".md-add2basket-list").slick({
                slidesToShow: 4,
                slidesToScroll: 4,
                autoplay: false,
                autoplaySpeed: 2000,
                infinite: false,
                responsive: [
                    {
                        breakpoint: 900,
                        settings: {
                            slidesToShow: 3,
                            slidesToScroll: 3,
                            arrows: false,
                        },
                    },
                    {
                        breakpoint: 760,
                        settings: {
                            slidesToShow: 2,
                            slidesToScroll: 2,
                            arrows: false,
                        },
                    }
                ],
            });
        
    }

    function getHighPriceHtml(item) {
        if (item.high_price && parseFloat(item.high_price) > parseFloat(item.price)) {
            return `<small style="text-decoration: line-through; color: #a1a1a1; padding-left: 5px;">${formatPrice(decorePrice(item.high_price), priceTemplate)}</small>`;
        }
        return '';
    }

    function getShippingInfo() {
        let productId = $("#productId").val();
        let productSkuId = $("#productSkuId").val();
        let quantityAddCart = $("#quantity").val();
        if (!quantityAddCart) {
            quantityAddCart = 1;
        }

        $.ajax({
            url: '/shipping/info?id=' + productId + "&sku=" + productSkuId + "&quantity=" + quantityAddCart
        }).done(function(response) {
            if (response.status == 'successful') {
                $(".product-delivery").show();

                const shippingInfo = response.result;
                const countryName = response.countryName;

                $(".delivery-country-name").text(countryName);

                let htmlShippingCalculate = '';
                let htmlCostToShip = '';
                let fee = 0;
                let index = 0;
                let isSame = true;

                for (const [key, value] of Object.entries(shippingInfo)) {
                    if (fee === 0 && value.shipping_fee > 0) {
                        fee = value.shipping_fee;
                    } else {
                        if (fee !== value.shipping_fee) {
                            isSame = false;
                        }
                    }
                    index ++;
                }

                if (index === 1) {
                    isSame = false;
                }

                for (const [key, value] of Object.entries(shippingInfo)) {

                    let minDate = new Date();
                    let maxDate = new Date();
                    minDate.setDate(minDate.getDate() + parseInt(value.default_min_time));
                    maxDate.setDate(maxDate.getDate() + parseInt(value.default_max_time));

                    let text = shippingInfoTemplate({
                        minDate,
                        maxDate,
                        isSame,
                        key,
                        value
                    });

                    let textFee = shippingInfoShippingFeeTemplate({
                        key,
                        value
                    });

                    htmlShippingCalculate += text
                    htmlCostToShip += textFee;

                    if (value.additional_info && value.additional_info.length > 0) {
                        let additionalInfo = value.additional_info[0];
                        if (parseFloat(additionalInfo.fee_limit) > 0) {
                            $("#product-fee-if-limit").html(`<span style="font-weight: 500;">Free Shipping</span>` + " on orders over " + `<span style="font-weight: 500;">` + formatPrice(decorePrice(parseFloat(additionalInfo.fee_limit)), priceTemplate) + "</span> " + `(${additionalInfo.cate_name})`)
                        }
                        if (parseFloat(additionalInfo.adding_fee) == 0 && additionalInfo.cate_name == "Stickers") {
                            $("#product-free-second").html(`<span style="font-weight: 500;">No extra shipping fee</span>` + " for additional items " + `(${additionalInfo.cate_name})`)
                        }
                    }
                }
                htmlShippingCalculate += 
                `<p style="margin: 14px 0; font-size: 14px; font-weight: 400;">
                    <span style="text-transform: capitalize; font-weight: 500;">
                    Ready to ship in:
                    </span>
                    1 business day
                </p>`;
                $(".shipping-info-calculate").html(htmlShippingCalculate);
                $(".shipping-info-shipping-cost").html(htmlCostToShip);

            }
        })
    }

    function shippingInfoTemplate(data) {
        let shippingTemplate = 'ptv';

        if (typeof window.shippingTemplate !== 'undefined') {
            shippingTemplate = window.shippingTemplate;
        }

        switch (shippingTemplate) {
            case 'ptv':
                return ptvShippingTemplate(data);
            case 'et':
                return etShippingTemplate(data)
        }
    }

    function shippingInfoShippingFeeTemplate(data) {
        return `<span style="display: block">
                    ${ formatPrice(decorePrice(data.value.shipping_fee), priceTemplate) }
                </span>`;
    }

    function ptvShippingTemplate(data)
    {
        let textTemplate = "#key between #from - #to";
        textTemplate = textTemplate.replace('#key', data.key + '</span>');
        textTemplate = textTemplate.replace('#from', `${monthNames[data.minDate.getMonth()].replace("#date", data.minDate.getDate())}`);
        textTemplate = textTemplate.replace('#to', `${monthNames[data.maxDate.getMonth()].replace("#date", data.maxDate.getDate())}`);

        let text = '';
        if (!data.isSame) {
            if (data.value.shipping_fee && data.value.shipping_fee > 0) {
                text = formatPrice(decorePrice(data.value.shipping_fee), priceTemplate) + " ";
            }
            if (data.value.shipping_fee === 0) {
                text = `Free `;
            }
        }
        textTemplate = text + textTemplate;

        return `<p style="margin: 14px 0; font-size: 14px; font-weight: 400;">
                    <span style="text-transform: capitalize; font-weight: 500;">
                    ${textTemplate}
                </p>`;
    }

    function etShippingTemplate(data) {

        let textTemplate = "#from - #to";
        textTemplate = textTemplate.replace('#from', `${monthNames[data.minDate.getMonth()].replace("#date", data.minDate.getDate())}`);
        textTemplate = textTemplate.replace('#to', `${monthNames[data.maxDate.getMonth()].replace("#date", data.maxDate.getDate())}`);

        return `<span style="display: block">
                    ${textTemplate}
                </span>`;
    }

    function generateSlider() {
        a = document.querySelector('#add2basket-close')
        b = document.querySelector('.add2basket-wrapper');

        if (window.innerWidth > 1025) {
            function add2basketSlider (selector, config) {
                var
                _mainElement = document.querySelector(selector),
                _sliderWrapper = _mainElement.querySelector('.md-add2basket-list'),
                _sliderItems = _mainElement.querySelectorAll('.md-add2basket-item'),
                _sliderControls = _mainElement.querySelectorAll('.md-add2basket-slider-control'),
                _sliderControlLeft = _mainElement.querySelector('.md-add2basket-slider-control_left'),
                _sliderControlRight = _mainElement.querySelector('.md-add2basket-slider-control_right'),
                _wrapperWidth = parseFloat(getComputedStyle(_sliderWrapper).width),
                _itemWidth = parseFloat(getComputedStyle(_sliderItems[0]).width),
                _positionLeftItem = 0,
                _transform = 0,
                _step = _itemWidth / _wrapperWidth * 100,
                _items = [];


                _sliderItems.forEach(function (item, index) {
                    _items.push({ item: item, position: index, transform: 0 });
                });

                var position = {
                    getItemMin: function () {
                        var indexItem = 0;
                        _items.forEach(function (item, index) {
                            if (item.position < _items[indexItem].position) {
                                indexItem = index;
                            }
                        });
                        return indexItem;
                    },
                    getItemMax: function () {
                        var indexItem = 0;
                        _items.forEach(function (item, index) {
                            if (item.position > _items[indexItem].position) {
                                indexItem = index;
                            }
                        });
                        return indexItem;
                    },
                    getMin: function () {
                        return _items[position.getItemMin()].position;
                    },
                    getMax: function () {
                        return _items[position.getItemMax()].position;
                    }
                }

                var _transformItem = function (direction) {
                    var nextItem;
                    if (direction === 'right') {
                        if (_positionLeftItem + 1 > _items.length) {
                            return;
                        };
                        _positionLeftItem++;
                        if ((_positionLeftItem + _wrapperWidth / _itemWidth - 1) > position.getMax()) {
                            nextItem = position.getItemMin();
                            _items[nextItem].position = position.getMax() + 1;
                            _items[nextItem].transform += _items.length * 100;
                            // _items[nextItem].item.style.transform = 'translateX(' + _items[nextItem].transform + '%)';
                        }
                        _sliderControlLeft.style.display = 'flex'
                        if (_positionLeftItem > _items.length - 4) {
                            return;
                        };
                        if (_positionLeftItem == _items.length - 4) {
                            _sliderControlRight.style.display = 'none'
                        };
                        _transform -= _step;

                    }
                    if (direction === 'left') {

                        if (_positionLeftItem - 1 < 0) {
                            return;
                        };
                        _positionLeftItem--;
                        if (_positionLeftItem < position.getMin()) {
                            nextItem = position.getItemMax();
                            _items[nextItem].position = position.getMin() - 1;
                            _items[nextItem].transform -= _items.length * 100;
                            // _items[nextItem].item.style.transform = 'translateX(' + _items[nextItem].transform + '%)';
                        }
                        _sliderControlRight.style.display = 'flex'

                        if (_positionLeftItem < 0) {
                            return;
                        };
                        if (_positionLeftItem < 1) {
                            _sliderControlLeft.style.display = 'none'
                        };

                        _transform += _step;

                    }
                    _sliderWrapper.style.transform = 'translateX(' + _transform + '%)';
                }



                var _controlClick = function (e) {
                    var direction = this.classList.contains('md-add2basket-slider-control_right') ? 'right' : 'left';
                    e.preventDefault();
                    _transformItem(direction);
                };

                var _setUpListeners = function () {
                    _sliderControls.forEach(function (item) {
                        item.addEventListener('click', _controlClick);
                    });
                }

                _setUpListeners();
                return {
                    right: function () {
                        _transformItem('right');
                    },
                    left: function () {
                        _transformItem('left');
                    }
                }
            }
            var slider = add2basketSlider('.md-add2basket-slide')
        }
    }
    function decorePrice(price) {
        if (priceConfig) {
            price = (parseFloat(price) + parseFloat(priceConfig["adding_price"])) * parseFloat(priceConfig["ratio"]);
            price = decimalAdjust('ceil', price);
        }
        return price;
    }
    function decimalAdjust(type, value) {
        let matches = priceTemplate.match(/{money}{([^a-zA-z0-9]+)}{([0-9]+)}/);
        let exp = -2;
        if (matches.length == 3) {
            exp = parseInt(matches[2] * (matches[2] ? -1 : 1));
        }
        // If the exp is undefined or zero...
        if (typeof exp === 'undefined' || +exp === 0) {
        return Math[type](value);
        }
        value = +value;
        exp = +exp;
        // If the value is not a number or the exp is not an integer...
        if (isNaN(value) || !(typeof exp === 'number' && exp % 1 === 0)) {
            return NaN;
        }
        // Shift
        value = value.toString().split('e');
        value = Math[type](+(value[0] + 'e' + (value[1] ? (+value[1] - exp) : -exp)));
        // Shift back
        value = value.toString().split('e');
        return +(value[0] + 'e' + (value[1] ? (+value[1] + exp) : exp));
    }
	</script>
	<script src="https://assets.printerval.com/assets/js/controllers/sku-controller.js?v=20230802003012"></script>
	<script>
		var productPageModule = angular.module('productPageModule', ["ngSanitize"]);
        angular.element(document).ready(function() {
            let appElement = document.getElementsByClassName('product-page-content');
            if(appElement && appElement.length) appElement = appElement[0];

            angular.bootstrap(appElement, ['productPageModule']);
        });

        productPageModule.directive('lazyLoad', function () {

            let loadImgSrc = function (target, observer = null)
            {
                let checkNeedContinueObserve = true;

                if (target.parentElement.tagName === 'PICTURE') {
                    let sources = target.parentElement.getElementsByTagName('source');
                    for (let i = 0; i < sources.length; i++) {
                        if (sources[i].getAttribute('data-srcset')) {
                            sources[i].srcset = sources[i].getAttribute('data-srcset');
                            checkNeedContinueObserve = false;
                        }
                    }
                } else if (target.dataset.src) {
                    target.src = target.dataset.src;
                    checkNeedContinueObserve = false;
                }

                if (!checkNeedContinueObserve && observer) {
                    observer.unobserve(target);
                }
            }

            return {
                restrict: 'A',
                link: function (scope, element) {

                    if (!'IntersectionObserver' in window &&
                        !'IntersectionObserverEntry' in window &&
                        !'intersectionRatio' in window.IntersectionObserverEntry.prototype) {

                        function loadImageDirect(img, interval = null) {
                            if (img.parentElement.tagName === 'PICTURE') {
                                let sources = img.parentElement.getElementsByTagName('source');
                                for (let i = 0; i < sources.length; i++) {
                                    let url = sources[i].getAttribute('data-srcset');
                                    if (!url.includes('{\{')) {
                                        sources[i].srcset = url;
                                        if (interval) {
                                            clearInterval(interval);
                                        }
                                    }
                                }
                            } else if (img.dataset.src && !img.dataset.src.includes('{\{')) {
                                img.src = img.dataset.src;
                                if (interval) {
                                    clearInterval(interval);
                                }
                            }
                        }

                        const img = angular.element(element)[0];
                        let interval = setInterval(function () {
                            loadImageDirect(img, interval);
                        }, 300);
                        loadImageDirect(img);

                    } else {
                        const observer = new IntersectionObserver(loadImg, {
                            rootMargin: "100px",
                            root: null,
                            threshold: 0.1
                        });

                        const img = angular.element(element)[0];
                        observer.observe(img);
                        function loadImg(changes) {
                            changes.forEach(change => {
                                if (change.intersectionRatio > 0) {
                                    loadImgSrc(change.target, observer);
                                }
                            });
                        }
                    }
                }
            };
        });

        try {
            if (window.dataLayer) {
                window.dataLayer.push({
                    content_type: 'product',
                    content_ids: ["P623022-BLA-M-MEN-07"],
                    value: "15.95",
                    contents: [{"id":"P623022-BLA-M-MEN-07","quantity":1}],
                    category_name: "Custom T-Shirts",
                    content_name: "Disney Family Vacation 2023 Customized T-Shirt Disney Squad",
                    currency: "USD"
                });
                window.dataLayer.push({
                    'dr_event_type': 'view_item',
                    'dr_items':[{id:'P623022-BLA-M-MEN-07',google_business_vertical: 'retail' }],
                    'event':'dynamic_remarketing'
                });
                                window.dataLayer.push({
                    'criteo_items':[{"id":"US623022","quantity":1,"price":"15.95"}]
                });
                window.dataLayer.push({
                    'criteo_product_id':'US623022'
                });
                window.dataLayer.push({'event': 'view_content'});
                fbq('track', 'ViewContent',{'content_ids':
                            ["P623022-BLA-M-MEN-07"]
                },{'eventID':"_1690938654"});
            }
        } catch (err) {
            console.log(err);
        }
