					<script>
						var defaultCurrencySwitch = 'USD';
    var defaultCountrySelect = '/';
    var currencySwitch = defaultCurrencySwitch;
    var countrySelect = defaultCountrySelect;
    var defaultTextCurrencySwitch = '$ US Dollar (USD)';
    var defaultTextCountrySwitch = 'US and Others';
    var defaultImageCountrySwitch = 'https://printerval.com/modules/localization/images/us.svg';
    function openPopup() {
        currencySwitch = defaultCurrencySwitch;
        countrySelect = defaultCountrySelect;
        document.getElementById('select-group-option').classList.add("active")
    }
    function closePopup() {
        document.getElementById('select-group-option').classList.remove("active");
        currencySwitch = defaultCurrencySwitch;
        countrySelect = defaultCountrySelect;
        document.getElementById("currency-inner-text").textContent = defaultTextCurrencySwitch;
        document.getElementById("country-inner-text").textContent = defaultTextCountrySwitch;
        document.getElementById("image-switch-country").src = defaultImageCountrySwitch;
    }
    function openSelectCountry(countLocale) {
        if (countLocale > 1) {
            document.getElementById('switch-item-country').classList.add("active")
        }
    }
    function openSelectCurrency() {
        document.getElementById('switch-item-currency').classList.add("active")
    }

    function selectCountry(text, img, url) {
        countrySelect = url;
        document.getElementById('switch-item-country').classList.remove("active")
        document.getElementById("country-inner-text").textContent = text;
        document.getElementById("image-switch-country").src = img;
    }
    function selectCurrency(currency, text) {
        currencySwitch = currency;
        document.getElementById('switch-item-currency').classList.remove("active")
        document.getElementById("currency-inner-text").textContent = text;
    }
    function saveSwitch() {
        setCookie("currency", currencySwitch);
        if (countrySelect == defaultCountrySelect) {
            window.location.reload();
        } else {
            window.location = countrySelect;
        }
    }

    function setCookie(name, value, days) {
        var expires = "";
        if (days) {
            var date = new Date();
            date.setTime(date.getTime() + (days*24*60*60*1000));
            expires = "; expires=" + date.toUTCString();
        }
        document.cookie = name + "=" + (value || "")  + expires + "; path=/";
    }

    window.addEventListener('click', function(e){   
        if (document.getElementById('switch-item-currency')) {
            if (document.getElementById('switch-item-currency').contains(e.target) || document.getElementById('switch-item-country').contains(e.target)) {
                if (document.getElementById('switch-item-country').contains(e.target)) {
                    document.getElementById('switch-item-currency').classList.remove("active")
                }
                if (document.getElementById('switch-item-currency').contains(e.target)) {
                    document.getElementById('switch-item-country').classList.remove("active")
                }
                // Clicked in box
            } else {
                // Clicked outside the box
                document.getElementById('switch-item-country').classList.remove("active")
                document.getElementById('switch-item-currency').classList.remove("active")
            }
        }
    });

					</script>
